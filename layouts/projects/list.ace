= content nav
  {{ partial "nav" . }}
= content main
  .font-sans.text-lg.text-grey-darkest.spaced-y-6
    {{.Content}}
    ul.list-reset.mt-16.spaced-y-8
      {{ range sort $.Site.Data.projects "date" "desc" }}
        .projects
          {{ partial "portfolio-item" . }}
      {{ end }}
= content footer
  {{ partial "footer" . }}
